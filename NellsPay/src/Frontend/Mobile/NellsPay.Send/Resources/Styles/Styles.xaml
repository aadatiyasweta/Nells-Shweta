    <Style x:Key="BodySemiBold14NotificationActive"
            TargetType="Label">
        <Setter Property="FontFamily"
                Value="FSemBold"/>
        <Setter Property="FontSize"
                Value="14"/>
        <Setter Property="LineHeight"
                Value="1.43"/>
        <Setter Property="TextColor"
                Value="{AppThemeBinding Light={DynamicResource AccentWhiteLight}, Dark={DynamicResource AccentWhiteDark}}"/>
    </Style>
    <Style x:Key="BodySemiBold14NotificationInActive"
            TargetType="Label">
        <Setter Property="FontFamily"
                Value="FSemBold"/>
        <Setter Property="FontSize"
                Value="14"/>
        <Setter Property="LineHeight"
                Value="1.43"/>
        <Setter Property="TextColor"
                Value="{AppThemeBinding Light={DynamicResource Text2Light},
                                                            Dark={DynamicResource Text2Dark}}"/>
    </Style>



    <Style x:Key="BaseBorderStyleNotificationInActive"
            TargetType="Border">
        <Setter Property="Background"
                Value="{AppThemeBinding Light={DynamicResource Neutral50Light},
                                                             Dark={DynamicResource Neutral50Dark}}"/>
        <Setter Property="StrokeShape">
            <Setter.Value>
                <RoundRectangle CornerRadius="12"/>
            </Setter.Value>
        </Setter>
        <Setter Property="StrokeThickness"
                Value="1"/>
        <Setter Property="Stroke"
                Value="{AppThemeBinding Light={DynamicResource Neutral300Light},
                                                         Dark={DynamicResource Neutral300Dark}}"/>
        <Setter Property="Padding"
                Value="0"/>
    </Style>

    <Style x:Key="BaseBorderStyleNotificationActive"
            TargetType="Border">
        <Setter Property="Background"
                Value="{AppThemeBinding Light={DynamicResource Neutral800Light},
                                                             Dark={DynamicResource Neutral800Dark}}"/>
        <Setter Property="StrokeShape">
            <Setter.Value>
                <RoundRectangle CornerRadius="12"/>
            </Setter.Value>
        </Setter>
        <Setter Property="StrokeThickness"
                Value="0"/>
        <Setter Property="Padding"
                Value="0"/>
    </Style>








